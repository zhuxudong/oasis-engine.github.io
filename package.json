    "@oasis-engine/stats": "alpha",
    "@oasis-engine/touch": "^0.0.16",
    "@stackblitz/sdk": "^1.5.2",
    "@tweenjs/tween.js": "^18.6.4",
    "antd": "4.15.6",
    "cheerio": "^1.0.0-rc.3",
