    "node-fetch": "^2.5.0",
    "nprogress": "^0.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.0",
    "prismjs": "^1.15.0",
    "sass": "^1.35.1",
    "slash2": "^2.0.0",
    "stylelint": "^13.6.0",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.3",
