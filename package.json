{
  "name": "oasis-engine-site",
  "version": "0.5.0",
  "description": "site of Oasis Engine",
  "homepage": "http://oasisengine.cn",
  "repository": {
    "type": "git",
    "url": "https://github.com/oasis-engine/oasis-engine.github.io"
  },
  "license": "MIT",
  "scripts": {
    "bootstrap": "npm run typedoc",
    "build": "gatsby clean && gatsby build --prefix-paths",
    "dev": "gatsby clean && gatsby develop",
    "lint": "npm run lint:js && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src --fix",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "prettier": "prettier --write \"**/**\"",
    "serve": "gatsby serve",
    "presite": "npm run build",
    "start": "gatsby develop",
    "test": "npm run lint",
    "typedoc": "node scripts/typedoc/generateTS.js",
    "playground": "vite serve playground/.dev --config playground/.dev/vite.config.js"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
